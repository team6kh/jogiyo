	cart_ui = cart_ui +                                 '<span class="opt-open" id="optopen_'+ seq +'" onclick="toggleLayer(\'menu_'+ seq +'\'); fn_opt_fold('+seq+'); return false;">열기</span>';
	cart_ui = cart_ui +                             '</div>';
	cart_ui = cart_ui +                         '</div>';
	cart_ui = cart_ui +                     '</div>';
	cart_ui = cart_ui +                     '<div class="menu-det menu_'+ seq +'">';

	for (var k in price_Grp_Nm )
