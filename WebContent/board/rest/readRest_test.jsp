//------------------------------------------------------------------------------
//PURPOSE :	모바일일 경우 IOS 6이상 Android 2.2이상이 아니면 이미지 변경버튼 숨김
//CREATE  :	2014-01-07 태승호
//MODIFY  :
//------------------------------------------------------------------------------
function fn_getOS()
{
	var ua = navigator.userAgent;
	var uaindex;

	// determine OS
	if ( ua.match(/iPad/i) || ua.match(/iPhone/i) )
	{
		userOS = 'iOS';
		uaindex = ua.indexOf( 'OS ' );
	}
	else if ( ua.match(/Android/i) )
	{
		userOS = 'Android';
		uaindex = ua.indexOf( 'Android ' );
	}
	else
	{
		userOS = 'unknown';
	}

	// determine version
	if ( userOS === 'iOS'  &&  uaindex > -1 )
	{
		userOSver = ua.substr( uaindex + 3, 3 ).replace( '_', '.' );
	}
	else if ( userOS === 'Android'  &&  uaindex > -1 )
	{
		userOSver = ua.substr( uaindex + 8, 3 );
	}
	else
	{
		userOSver = 'unknown';
	}

	if ( userOS === 'iOS' && Number( userOSver.charAt(0) ) < 6 ) {
		$("input[type=file]").hide();
		$("#label_file").hide();

	}else if( userOS === 'Android' && Number( userOSver.charAt(0) ) < 2.2 ) {
		$("input[type=file]").hide();
		$("#label_file").hide();
	}else{

	}
}

