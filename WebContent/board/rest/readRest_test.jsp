	cart_ui = cart_ui +                             '</div>';
	cart_ui = cart_ui +                             '<div class="price">';
	cart_ui = cart_ui +                             '<input type="hidden" id="cart_price_'+ shopNo +'_' + memNo + '_' + seq + '" value="'+ price +'">';
