	if ( tot_price < minordprice )
	{
		alert("최소 주문금액은 " + minordprice + "원 입니다.");
		return;
	}
	else if ( tot_price == '0' )
	{
		alert("0원은 주문이 불가능합니다.");
		return;
	}
	else
	{
