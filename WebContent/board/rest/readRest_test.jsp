function fn_item_to_string(seq) {

	var memNo    = $('#mem_No').val();
	var shopNo   = $('#shopNo').val();
	var cartId    = "cart_" + shopNo + "_" + memNo;

	var selected_item_grp = "";
	var item_cnt = "";

