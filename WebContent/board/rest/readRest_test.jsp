			selected_item_grp += ("^" + $(this).val());
		}
	});

	//체크박스에서 선택 된 메뉴 가져오기
	$(":input:checkbox[id*=opt_" + seq + "_]:checked").each(function(){

		if (selected_item_grp.length == 0)
		{
			selected_item_grp = $(this).val();
		}
		else
		{
			selected_item_grp += ("^" + $(this).val());
		}
	});

	item_cnt = seq + "|" + "1";   //처음에는 1개씩만 선택
	fn_set_cart(shopNo,memNo,selected_item_grp,item_cnt);
}

