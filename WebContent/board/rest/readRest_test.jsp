function fn_set_cart(shopNo,memNo,selected_item_grp,item_cnt) {

	$.ajax({
		type: 'POST',
		async: true,
		url: '/shop/set_cart',
		data: {
				 shopNo:shopNo
				,memNo:memNo
				,selected_item_grp:selected_item_grp
				,item_cnt:item_cnt
		},
		success: function(data) {
			if (!data) return;

			if ( $.trim(data) == "set_cart" )
			{
				if ( $("li[id^='cart_item_']").length == 0 )
				{
					fn_cart_ui();
				}
				else
				{
					fn_set_cart_UI(shopNo,memNo,selected_item_grp,item_cnt);
